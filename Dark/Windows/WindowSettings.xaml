<Window x:Class="OxyPlot.Dark.Wpf.WindowSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OxyPlot.Dark.Wpf"
        mc:Ignorable="d" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner"
        Title="Settings" Height="200" Width="300" MinHeight="200" MinWidth="300" MaxHeight="200" MaxWidth="300">
    <Grid>
        <CheckBox Name="AdjustY" Content="Adjust Y" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" IsThreeState="False"/>
        <Button Name="ButtonOK" Content="OK" Margin="0,0,5,5" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" 
                Click="ButtonOK_Click"  IsDefault="True"/>
    </Grid>
</Window>
